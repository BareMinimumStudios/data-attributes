{
    "schemaVersion": 1,
    "id": "dataattributes",
    "version": "${version}",
    "name": "Data Attributes",
    "description": "Overhauls the entity attributes system and exposes it with datapacks.",
    "authors": [
      "CleverNucleus"
    ],
    "contact": {
        "homepage": "https://github.com/CleverNucleus/DataAttributes",
        "sources": "https://github.com/CleverNucleus/DataAttributes"
    },
    
    "license": "MIT",
    "icon": "assets/dataattributes/icon.png",
    
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.github.clevernucleus.dataattributes.DataAttributes"
        ],
        "client": [
            "com.github.clevernucleus.dataattributes.client.DataAttributesClient"
        ]
    },
    "mixins": [
        "dataattributes.mixins.json"
    ],
    
    "depends": {
        "fabricloader": ">=0.11.3",
        "fabric": "0.40.1",
        "minecraft": "1.17.x",
        "java": ">=16"
    }
}
